{
    "path": {
        "www/content/sass/style.scss": {
            "sass.enabled": true,
            "sass.options": {
                "outputDir": "../css/",
                "imagePath": null,
                "sourceComments": "map",
                "outputStyle": "nested"
            },
            "linting.collapsed": true
        },
        "www/materialize/*.scss": {
            "sass.enabled": false,
            "linting.collapsed": false
        }
    }
}